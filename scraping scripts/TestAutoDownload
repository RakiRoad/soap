#Testing auto download of file;

require 'open-uri'
require 'csv'
url =  "http://ofmpub.epa.gov/enviro/efservice/MV_TRI_BASIC_DOWNLOAD/st/=/NJ/year/=/2014/fname/TRI_2014_NJ.csv/CSV"

download = open(url)
IO.copy_stream(download, 'Njdata.csv')
CSV.new(download).each do |l|
   puts 1
   puts file downloaded
end
